{
    "version": 2,
    "builds": [
      {
        "src": "server/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "src/package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "src/build" }
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "server/index.js" },
      { "src": "/(.*)", "dest": "src/build/$1" }
    ]
  }
  